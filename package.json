{
  "name": "redux-api-middleware",
  "version": "0.2.0",
  "description": "Redux middleware for calling an API.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib coverage",
    "cover": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./test/index.js",
    "lint": "./node_modules/.bin/eslint src test",
    "prepublish": "npm run lint && npm run clean && npm test && npm run build",
    "test": "./node_modules/.bin/babel-node test/index.js | node_modules/.bin/tap-spec"
  },
  "repository": "agraboso/redux-api-middleware",
  "homepage": "https://github.com/agraboso/redux-api-middleware",
  "keywords": [
    "redux",
    "api",
    "middleware",
    "redux-middleware",
    "flux"
  ],
  "author": {
    "name": "Alberto Garcia-Raboso",
    "email": "agraboso@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.1.1",
    "lodash.isplainobject": "^3.2.0",
    "normalizr": "^0.1.3"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-istanbul": "^0.3.17",
    "eslint": "^1.1.0",
    "nock": "^2.10.0",
    "rimraf": "^2.4.2",
    "tap-spec": "^4.0.2",
    "tape": "^4.2.0"
  }
}
